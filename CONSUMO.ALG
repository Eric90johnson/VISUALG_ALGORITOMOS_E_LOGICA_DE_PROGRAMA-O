Algoritmo "Consumo"

Var

   distancia, combustivel, consumo : real


Inicio

   escreval("Distância percorrida: ")
   leia(distancia)
   
   escreval()
   
   escreval("Combustivel gasto: ")
   leia(combustivel)
   
   escreval()
   
   consumo <- distancia / combustivel

   escreval("CONSUMO MÉDIO É IGUAL ", consumo:5:3)



   



   

Fimalgoritmo